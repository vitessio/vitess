     "id": "coordinate.jobs.insert",
     "path": "teams/{teamId}/jobs",
     "httpMethod": "POST",
     "description": "Inserts a new job. Only the state field of the job should be set.",
     "parameters": {
      "address": {
       "type": "string",
       "description": "Job address as newline (Unix) separated string",
       "required": true,
       "location": "query"
