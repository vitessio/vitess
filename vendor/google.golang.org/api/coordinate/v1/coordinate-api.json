    "items": {
     "type": "array",
     "description": "Jobs in the collection.",
     "items": {
      "$ref": "Job"
     }
    },
    "kind": {
     "type": "string",
     "description": "Identifies this object as a list of jobs.",
     "default": "coordinate#jobList"
    },
    "nextPageToken": {
     "type": "string",
     "description": "A token to provide to get the next page of results."
    }
   }
  },
