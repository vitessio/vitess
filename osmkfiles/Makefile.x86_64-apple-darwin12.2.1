	${SB} -m PyXML -ip ${ADDON_DIR}/pbutils-1.2.0/sb-db.xml

${DIST_PREFIX}/${CPU}/MOTOpbutils12-1.2.0-1.${CPU}.rpm:
	${PACKAGE_BUILD} -1 ${ADDON_DIR}/pbutils-1.2.0/pb-db.xml
pbutils-1.2.0-gen-pkg-inst: pbutils-1.2.0-gen-pkg-inst-remote
	${GPD}  ${ADDON_DIR}/pbutils-1.2.0/pb-db.xml >> ${DIST_PREFIX}/${CPU}/pkg-db.xml
	(cd ${DIST_PREFIX}/${CPU};zip  data.zip  MOTOpbutils12* ;${M5S}  data.zip |awk '{print $$1}' > data.zip.md5)
	(cd ${DIST_PREFIX}/${CPU};zip -r  pbutils-1.2.0.${CPU}.rpm.pkg-inst pkg-db.xml data.zip data.zip.md5;${M5S}  pbutils-1.2.0.${CPU}.rpm.pkg-inst  |awk '{print $$1}' > pbutils-1.2.0.${CPU}.rpm.pkg-inst.md5)
	cd ${DIST_PREFIX}/${CPU};rm -f pkg-db.xml data.zip data.zip.md5

pbutils-1.2.0-upload: 
	cd ${DIST_PREFIX}/${CPU};${RSYNC} -azpv   pbutils-1.2.0.${CPU}.rpm.pkg-inst pbutils-1.2.0.${CPU}.rpm.pkg-inst.md5 ${PACKAGE_UPLOAD_PATH}/${ARCHI}/
	cd ${DIST_PREFIX}/${CPU};rm -f ${DIST_PREFIX}/${CPU}/pkg-db.xml pbutils-1.2.0.${CPU}.rpm.pkg-inst pbutils-1.2.0.${CPU}.rpm.pkg-inst.md5

pbutils-1.2.0-clean: 
	rm -rf ${IPFX}/pbutils12 ${SB_DB}/pbutils-1.2.0 


pbutils-1.2.0-lpms-gen: 
	(cd ${DIST_PREFIX}/${CPU};tar cf MOTOpbutils-1.2.0.${CPU}.rpm.tar MOTOpkgutils15*)
pbutils-1.2.0-lpms-upload:  pbutils-1.2.0-lpms-gen
	cd ${DIST_PREFIX}/${CPU};${RSYNC}   MOTOpbutils-1.2.0.${CPU}.rpm.tar ${PACKAGE_UPLOAD_PATH}/${ARCHI}/
	cd ${DIST_PREFIX}/${CPU};rm -f ${DIST_PREFIX}/${CPU}/MOTOpbutils-1.2.0.${CPU}.rpm.tar

