	${SB} -m openssl ${ADDON_DIR}/pbutils-1.2.0/sb-db.xml
	${SB} -m openssl -ip ${ADDON_DIR}/pbutils-1.2.0/sb-db.xml
${IPFX}/pbutils12/lib/aux/python:
	${SB} -m python ${ADDON_DIR}/pbutils-1.2.0/sb-db.xml
	${SB} -m python -ip ${ADDON_DIR}/pbutils-1.2.0/sb-db.xml
${IPFX}/pbutils12/lib/aux/pythonp/lib/python2.3/site-packages/fchksum.so:
	${SB} -m python-fchksum ${ADDON_DIR}/pbutils-1.2.0/sb-db.xml
	${SB} -m python-fchksum -ip ${ADDON_DIR}/pbutils-1.2.0/sb-db.xml
